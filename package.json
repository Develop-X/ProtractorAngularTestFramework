{
  "name": "angularE2E",
  "version": "2.0.0",
  "description": "Angular E2E test",
  "scripts": {
    "pretest": "npm install",
    "webdriver-update": "webdriver-manager update",
    "e2e": "protractor config.js",
    "ci": "protractor ciconfig.js",
    "test": "npm run-script e2e || npm run-script report --continue-on-error",
    "travis": "npm run-script ci || npm run-script report --continue-on-error",
    "report": "allure generate allure-results --clean -o allure-report || true"
  },
  "keywords": [
    "Protractor",
    "Angular2"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
