{
  "name": "angular-E2E",
  "version": "2.0.0",
  "description": "Angular E2E test",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [
    "Protractor",
    "Angular"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "dependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.9.4",
    "@types/selenium-webdriver": "^3.0.10",
    "canonical-path": "0.0.2",
    "concurrently": "^4.0.1",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.1",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-protractor-runner": "^5.0.0",
    "grunt-run": "^0.8.1",
    "gulp": "^3.9.1",
    "gulp-angular-protractor": "^1.2.0",
    "gulp-protractor": "^4.1.0",
    "http-server": "^0.9.0",
    "jasmine": "^3.2.0",
    "jasmine-core": "~3.2.1",
    "jasmine-data-provider": "^2.2.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine2-protractor-utils": "^1.3.0",
    "jquery": "^3.3.1",
    "log4js-protractor-appender": "^1.1.2",
    "mkdirp": "^0.5.1",
    "protractor": "^5.4.1",
    "protractor-accessibility-plugin": "^0.3.0",
    "protractor-compass": "^1.0.0",
    "protractor-helpers": "^1.1.473",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "jasmine-allure-reporter": "^1.0.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.0.3",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2",
    "xlsjs": "^1.0.1",
    "xlsx": "^0.14.0",
    "xlsx-to-json": "^0.3.0"
  }
}
